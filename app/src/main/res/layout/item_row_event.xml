<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:tools="http://schemas.android.com/tools">    <de.hdodenhof.circleimageview.CircleImageView        android:layout_width="60dp"        android:layout_height="60dp"        android:layout_marginTop="10dp"        android:layout_marginLeft="10dp"        android:id="@+id/img_item_foto"        tools:src="@color/colorPrimary"        /><LinearLayout    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    android:layout_centerVertical="false"    android:layout_toRightOf="@+id/img_item_foto"    android:layout_toEndOf="@id/img_item_foto"    android:layout_marginTop="15dp"    android:layout_marginLeft="10dp"    >    <TextView        android:id="@+id/tv_item_nama"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginBottom="10dp"        android:textSize="16sp"        tools:text="@string/event_name" />    <TextView        android:id="@+id/tv_item_asal"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginBottom="10dp"        android:textSize="16sp"        tools:text="@string/from" /></LinearLayout></RelativeLayout>